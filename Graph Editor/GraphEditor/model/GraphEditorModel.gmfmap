<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="GraphEditorModel.ecore#//Graph/GraphNodes"/>
    <ownedChild>
      <domainMetaElement
          href="GraphEditorModel.ecore#//Node"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="GraphEditorModel.gmfgraph#NodeName"/>
        <features
            href="GraphEditorModel.ecore#//Node/Name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="GraphEditorModel.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="GraphEditorModel.gmfgraph#Node"/>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="GraphEditorModel.ecore#//Edge"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="GraphEditorModel.gmfgraph#EdgeValue"/>
      <features
          href="GraphEditorModel.ecore#//Edge/Value"/>
    </labelMappings>
    <containmentFeature
        href="GraphEditorModel.ecore#//Node/OutGoingEdges"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="GraphEditorModel.gmftool#//@palette/@tools.0/@tools.1"/>
    <diagramLink
        href="GraphEditorModel.gmfgraph#Edge"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="GraphEditorModel.ecore#//Edge/Source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="GraphEditorModel.ecore#//Edge/Target"/>
  </links>
  <diagram>
    <diagramCanvas
        href="GraphEditorModel.gmfgraph#grapheditormodel"/>
    <domainModel
        href="GraphEditorModel.ecore#/"/>
    <domainMetaElement
        href="GraphEditorModel.ecore#//Graph"/>
    <palette
        href="GraphEditorModel.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>

            priority="normal" 
            contentProvider="GraphEditor.graphmodel.diagram.navigator.GraphModelDomainNavigatorContentProvider" 
            labelProvider="GraphEditor.graphmodel.diagram.navigator.GraphModelDomainNavigatorLabelProvider"
            icon="icons/obj16/GraphmodelDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="graphmodel"/>
               </and>
               <instanceof value="GraphEditor.graphmodel.diagram.navigator.GraphModelDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="GraphEditor.graphmodel.diagram.navigator.GraphModelDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="GraphEditor.diagram.navigatorActionProvider"
            class="GraphEditor.graphmodel.diagram.navigator.GraphModelNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="GraphEditor.graphmodel.diagram.navigator.GraphModelAbstractNavigatorItem"/>
            </or>
         </enablement>
      </actionProvider>

            priority="normal" 
            contentProvider="GraphEditor.graphmodel.diagram.navigator.GraphModelNavigatorContentProvider" 
            labelProvider="GraphEditor.graphmodel.diagram.navigator.GraphModelNavigatorLabelProvider"
            icon="icons/obj16/GraphmodelDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="graphmodel_diagram"/>
               </and>
               <instanceof value="GraphEditor.graphmodel.diagram.navigator.GraphModelAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="GraphEditor.graphmodel.diagram.navigator.GraphModelAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="GraphEditor.diagram.navigatorSorter" 
               class="GraphEditor.graphmodel.diagram.navigator.GraphModelNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="graphmodel_diagram"/>
                  </and>
                  <instanceof value="GraphEditor.graphmodel.diagram.navigator.GraphModelAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="GraphEditor.diagram.domainResourceContent" 
